<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="biblioteca.modelo.dominio.mapeos" table="COPIASPELICULAS">
        <id name="codigo" type="integer">
            <column name="CODIGO"/>
            <generator class="increment"/>          
        </id>
        <property name="nombre" type="string">
            <column name="NOMBRE" length="20"/>
        </property>
        <property name="genero" type="string">
            <column name="GENERO" length="20"/>
        </property>
        <property name="precioAlquiler" type="double">
            <column name="PRECIOALQUILER" precision="5" scale="2"/>
        </property>
        <property name="estadoAlquilado" type="java.lang.Boolean">
            <column name="ESTADOALQUILADO" type="integer"/>
        </property>
        <property name="estado" type="java.lang.Boolean">
            <column name="ESTADO" type="integer"/>
        </property>
    </class>
</hibernate-mapping>
